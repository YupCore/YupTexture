{
  "version": 2,
  "configurePresets": [
    {
      "name": "x64-dbg",
      "displayName": "x64-Debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/out/build/x64-dbg",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
	{
	  "name": "x64-rel",
	  "displayName": "x64-Release",
	  "generator": "Ninja",
	  "binaryDir": "${sourceDir}/out/build/x64-rel",
	  "cacheVariables": {
		"CMAKE_BUILD_TYPE": "Release"
	  }
	},
	{
	  "name": "x64-rel-dbg",
	  "displayName": "x64-Release with PDB",
	  "generator": "Ninja",
	  "binaryDir": "${sourceDir}/out/build/x64-rel-dbg",
	  "cacheVariables": {
		"CMAKE_BUILD_TYPE": "RelWithDebInfo"
	  }
	}
  ]
}